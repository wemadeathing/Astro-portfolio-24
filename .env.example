# Web3Forms Configuration
WEB3FORMS_ACCESS_KEY=your_web3forms_access_key_here

# Resend Email Configuration (if using custom email API)
RESEND_API_KEY=your_resend_api_key_here
# Optional but recommended: use a verified sender/domain in Resend
RESEND_FROM_EMAIL=you@your-verified-domain.com
# Where submissions should be delivered (falls back to RESEND_FROM_EMAIL if unset)
CONTACT_EMAIL=your_contact_email@domain.com

# MailerLite (newsletter)
# Create a MailerLite API token and a subscriber group ID.
MAILERLITE_API_TOKEN=your_mailerlite_api_token_here
MAILERLITE_GROUP_ID=your_mailerlite_group_id_here
# Optional: override if MailerLite changes API base URL
MAILERLITE_API_BASE_URL=https://connect.mailerlite.com/api

# Chat assistant (Groq primary, Gemini fallback)
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
