# Web3Forms Configuration
WEB3FORMS_ACCESS_KEY=your_web3forms_access_key_here

# Resend Email Configuration (if using custom email API)
RESEND_API_KEY=your_resend_api_key_here
# Optional but recommended: use a verified sender/domain in Resend
